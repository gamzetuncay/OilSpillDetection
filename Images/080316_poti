// Study Area
var region010316 = ee.Geometry.Rectangle(
                [41.68959635214067, 41.76163098915713,
                40.995101907596016, 42.27757741759697], null, true);

var sar = ee.ImageCollection('COPERNICUS/S1_GRD')
    .filter(ee.Filter.listContains('transmitterReceiverPolarisation','VV'))
    .filter(ee.Filter.listContains('transmitterReceiverPolarisation','VH'))
    .filter(ee.Filter.eq('instrumentMode','IW'))
    .filter(ee.Filter.eq('orbitProperties_pass','DESCENDING'))
    .filterDate('2016-03-08','2016-03-10')
    .filterBounds(region010316)
    .mosaic()
    .clip(region010316);

print(sar);

var VV_Histogram = ui.Chart.image.histogram(sar.select('VV'), region010316, 100)
    .setSeriesNames(['VV'])
    .setOptions({
       hAxis: {title: 'dB'},
       vAxis: {title: 'Frequency'}});

print(VV_Histogram); 

Map.centerObject(region010316,8);

var imageVisParam = {"opacity":1,"bands":["VV"],"min":-40,"gamma":1}; 
Map.addLayer(sar,imageVisParam);

Export.image.toDrive({
  image: sar.visualize(imageVisParam),
  folder: 'Bitirme/görüntüler',
  description: '080316_poti',
  region: region010316,
  scale: 100
});

